{
  "name": "samil-hero",
  "private": true,
  "version": "0.12",
  "scripts": {
    "dev": "npm run && next dev -p 3000",
    "build": "tsc && next build",
    "build:int": "echo NEXT_PUBLIC_APP_VERSION=$npm_package_version >> .env.int && env-cmd -f .env.int next build",
    "start": "next start -p 3000",
    "test:ci": "npm run test -- --testResultsProcessor=\"jest-junit\" --watchAll=false --ci --coverage",
    "test:mr": "npm run test -- --testResultsProcessor=\"jest-sonar-reporter\" --watchAll=false --ci --coverage",
    "gen:error-code": "tsc generator/apiResponseCodeGenerator.ts && node generator/apiResponseCodeGenerator.js && rm generator/apiResponseCodeGenerator.js && rm src/data/index.js && rm src/data/**/*.js",
    "gen:prepare": "npm run gen:swagger && npm run gen:error-code",
    "changelog": "./changelog.sh",
    "scan:sonar": "sonar-scanner",
    "scan:lint": "next lint",
    "scan:all": "npm run scan:sonar; npm run scan:lint"
  },
